
public class StudentManager extends UserManager{
	
	public void add(Student student) {
		System.out.println(student.getFirstName()+" "+student.getLastName()+"kullanıcısı"+student.courses+"kursunu ekledi");
	}
	
	public void delete(Student student) {
		System.out.println(student.courses+"Kursu silindi");
	}
	


}
